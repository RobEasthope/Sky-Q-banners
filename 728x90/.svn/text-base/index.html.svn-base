<!DOCTYPE html>
<html lang="en">
<head>
<meta charset="utf-8">
<title>Banner</title>
    <style>
        #borderDiv {position: absolute; top: 0px; left: 0px; width: 970px; height: 250px; background-color: transparent; box-sizing: border-box; -moz-box-sizing: border-box; -webkit-box-sizing: border-box; border: 1px solid #999999; z-index: 402; pointer-events: none; }
        body { width: 100%; height: 100%; margin: auto; } 
        #prelaoder{ position:absolute; width:32px; height:auto; left:125px; top:100px; }
        #exitButton {position: absolute;width: 970px;height: 250px;top: 0px;left: 10px;cursor: pointer;background-color: rgba(0, 0, 0, 0);opacity: 0;z-index: 400;}
    </style>
    
    <link href="banner.css" rel="stylesheet">
   
    <script src="https://s0.2mdn.net/ads/studio/Enabler.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/headjs/1.0.3/head.load.min.js"></script>
    <script id="editable">
        
        //Enabler is initialized
          if (Enabler.isInitialized()) {
                enablerInitialized();
            } else {
                Enabler.addEventListener(studio.events.StudioEvent.INIT, enablerInitialized);
            }
        function enablerInitialized() {
            
             //Check for page load.
            if (Enabler.isPageLoaded()) {
                PoliteJSLOAD();
            } else {
                Enabler.addEventListener(studio.events.StudioEvent.PAGE_LOADED, PoliteJSLOAD);
            }
        }
        
        function PoliteJSLOAD() {
            head.load("banner.js");
        }
        
    </script>
    
</head>

<body>
    
<div class="bottom_container" id='bottom_container'></div>
<div class='middle_container' id='middle_container'></div>
<div><img src='loaderWheel.gif' id='preloader'></img></div>
<canvas id="canvas" width="728" height="90" style="background-color:#FFFFFF"></canvas>

<div class='canvasHolder'>
    <canvas id="testCanvas" width="728" height="90"></canvas>    
</div>
    
<div class='top_container' id='top_container'></div>
<button id="exitButton"></button>
<div id='legalsContainer'></div>
<div id='borderDiv'></div>

</body>
</html>
